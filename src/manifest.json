{
  "name": "Cliqz Privacy Warning",
  "version": "1.0",
  "description": "Cliqz Privacy Warning",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "<all_urls>",
    "cookies",
    "privacy",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "browser_style": true,
    "default_icon": {
      "16": "icons/png/16x16_consent-readonly-chrome.png",
      "24": "icons/png/24x24_consent-readonly-chrome.png",
      "32": "icons/png/16x16_consent-readonly-chrome.png",
      "19": "icons/svg/19x19_consent-readonly-cliqz.svg",
      "38": "icons/svg/38x38_consent-readonly-cliqz.svg"
    },
    "theme_icons": [
      {
        "light": "icons/svg/19x19_consent-readonly-cliqz-light.svg",
        "dark": "icons/svg/19x19_consent-readonly-cliqz-dark.svg",
        "size": 19
      },
      {
        "light": "icons/svg/38x38_consent-readonly-cliqz-light.svg",
        "dark": "icons/svg/38x38_consent-readonly-cliqz-dark.svg",
        "size": 38
      }
    ]
  },
  "web_accessible_resources": [
    "content-page-bridge.js"
  ]
}
